#[test]
fn test_init_game() {
    let (mastermind, _) = deploy_contract();
    let mut spy = spy_events();
    let creator_name = 'lukuluku';

    start_cheat_caller_address(mastermind.contract_address, PLAYER1);

    mastermind.register_player(creator_name);
    mastermind.init_game();

    stop_cheat_caller_address(mastermind.contract_address);

    assert!(mastermind.get_player_id(PLAYER1) == 0, "Incorrect player id");
    assert!(mastermind.get_player_name(PLAYER1) == creator_name, "Incorrect player name");
    assert!(mastermind.get_total_players_count() == 1, "Incorrect total number of players");
    assert!(mastermind.get_total_games_count() == 1, "Incorrect total number of games");
    assert!(
        mastermind.get_game_submitted_guesses(0, PLAYER1).len() == 0, "Incorrect guesses array",
