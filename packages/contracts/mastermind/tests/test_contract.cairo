        mastermind.get_game_solution_hash(0, PLAYER1) == solution_hash,
        "The committed hash should match the one onchain",
    );
    spy
        .assert_emitted(
            @array![
                (
                    mastermind.contract_address,
                    Mastermind::Event::RegisterPlayer(
                        Mastermind::RegisterPlayer { account: PLAYER1, player_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::InitializeGame(
                        Mastermind::InitializeGame { account: PLAYER1, game_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::CommitSolutionHash(
                        Mastermind::CommitSolutionHash { account: PLAYER1, solution_hash },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::OpponentJoined(
                        Mastermind::OpponentJoined { account: PLAYER2, game_id: 0 },
