    ContractClassTrait, DeclareResultTrait, EventSpyAssertionsTrait, declare, spy_events,
    start_cheat_caller_address, stop_cheat_caller_address,
};
use starknet::ContractAddress;

const PLAYER1: ContractAddress = 'PLAYER1'.try_into().unwrap();
const PLAYER2: ContractAddress = 'PLAYER2'.try_into().unwrap();

fn setup_guess(guess_arr: @Array<u8>, submitted: bool) -> Guess {
    let mut guess: Guess = Default::default();

    guess.g1 = *guess_arr[0];
    guess.g2 = *guess_arr[1];
    guess.g3 = *guess_arr[2];
    guess.g4 = *guess_arr[3];
    guess.submitted = submitted;

    guess
}

fn deploy_contract() -> (IMastermindDispatcher, IMastermindSafeDispatcher) {
    let contract_class = declare("Mastermind").unwrap().contract_class();

    let (contract_address, _) = contract_class.deploy(@array![]).unwrap();

    let mastermind = IMastermindDispatcher { contract_address };
    let safe_dispatcher = IMastermindSafeDispatcher { contract_address };

    (mastermind, safe_dispatcher)
}

#[test]
fn test_register_player() {
    let (mastermind, _) = deploy_contract();
    let mut spy = spy_events();
    let creator_name = 'lukuluku';

    start_cheat_caller_address(mastermind.contract_address, PLAYER1);

    mastermind.register_player(creator_name);

    stop_cheat_caller_address(mastermind.contract_address);

    assert!(mastermind.get_player_id(PLAYER1) == 0, "Incorrect player id");
    assert!(mastermind.get_player_name(PLAYER1) == creator_name, "Incorrect player name");
    assert!(mastermind.get_total_players_count() == 1, "Incorrect total number of players");
    spy
        .assert_emitted(
            @array![
                (
                    mastermind.contract_address,
                    Mastermind::Event::RegisterPlayer(
                        Mastermind::RegisterPlayer { account: PLAYER1, player_id: 0 },
                    ),
                ),
            ],
        )
}

