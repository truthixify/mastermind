#[test]
fn test_submit_guess() {
    let (mastermind, _) = deploy_contract();
    let mut spy = spy_events();
    let solution_hash1 = 0x265c6daf938dfc5d71bac7f6512e6fae06a430847ea1175df63b85a5332be1e1;
    let solution_hash2 = 0x00123456789;
    let player1_guess1 = array![65, 66, 67, 68];
    let player2_guess1 = array![80, 81, 82, 83];
    let creator_name = 'lukuluku';
    let opponent_name = 'lamidi';

    start_cheat_caller_address(mastermind.contract_address, PLAYER1);

    mastermind.register_player(creator_name);
    mastermind.init_game();
    mastermind.commit_solution_hash(0, solution_hash1);

    stop_cheat_caller_address(mastermind.contract_address);

    start_cheat_caller_address(mastermind.contract_address, PLAYER2);

    mastermind.register_player(opponent_name);
    mastermind.join_game(0);
    mastermind.commit_solution_hash(0, solution_hash2);

    stop_cheat_caller_address(mastermind.contract_address);

    start_cheat_caller_address(mastermind.contract_address, PLAYER1);

    mastermind.submit_guess(0, player1_guess1.clone());

    stop_cheat_caller_address(mastermind.contract_address);

    start_cheat_caller_address(mastermind.contract_address, PLAYER2);

    mastermind.submit_guess(0, player2_guess1.clone());

    stop_cheat_caller_address(mastermind.contract_address);

    assert!(mastermind.get_player_id(PLAYER1) == 0, "Incorrect player id");
    assert!(mastermind.get_total_players_count() == 2, "Incorrect total number of players");
    assert!(
        mastermind.get_game_solution_hash(0, PLAYER1) == solution_hash1,
        "The committed hash should match the one onchain",
    );
    assert!(
        mastermind.get_game_solution_hash(0, PLAYER2) == solution_hash2,
        "The committed hash should match the one onchain",
    );
    assert!(
        mastermind.get_game_current_stage(0) == Stages::Playing,
        "Game should be in playing
        stage",
    );
    assert!(
        mastermind
            .get_game_submitted_guesses(0, PLAYER1)
            .at(0) == @setup_guess(@player1_guess1, true),
        "Incorrect guesses array",
    );
    assert!(
        mastermind
            .get_game_submitted_guesses(0, PLAYER2)
            .at(0) == @setup_guess(@player2_guess1, true),
        "Incorrect guesses array",
    );
    spy
        .assert_emitted(
            @array![
                (
                    mastermind.contract_address,
                    Mastermind::Event::RegisterPlayer(
                        Mastermind::RegisterPlayer { account: PLAYER1, player_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::InitializeGame(
                        Mastermind::InitializeGame { account: PLAYER1, game_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::CommitSolutionHash(
                        Mastermind::CommitSolutionHash {
                            account: PLAYER1, solution_hash: solution_hash1,
                        },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::OpponentJoined(
                        Mastermind::OpponentJoined { account: PLAYER2, game_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::CommitSolutionHash(
                        Mastermind::CommitSolutionHash {
                            account: PLAYER2, solution_hash: solution_hash2,
                        },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::SubmitGuess(
                        Mastermind::SubmitGuess {
                            account: PLAYER1, current_round: 1, guess: player1_guess1,
                        },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::SubmitGuess(
                        Mastermind::SubmitGuess {
                            account: PLAYER2, current_round: 2, guess: player2_guess1,
                        },
                    ),
                ),
            ],
        )
}

