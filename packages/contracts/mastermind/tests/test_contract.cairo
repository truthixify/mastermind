    assert!(
        mastermind.get_game_submitted_hit_and_blow(0, PLAYER1).len() == 0,
        "Incorrect length of submitted hit and blow array",
    );
    assert!(mastermind.get_game_current_round(0) == 1, "Current round should be 1");
    assert!(
        mastermind.get_game_current_stage(0) == Stages::CreatorCommitSolutionHash,
        "Game stage should be commit solution hash stage",
    );

    spy
        .assert_emitted(
            @array![
                (
                    mastermind.contract_address,
                    Mastermind::Event::RegisterPlayer(
                        Mastermind::RegisterPlayer { account: PLAYER1, player_id: 0 },
                    ),
                ),
                (
                    mastermind.contract_address,
                    Mastermind::Event::InitializeGame(
                        Mastermind::InitializeGame { account: PLAYER1, game_id: 0 },
                    ),
                ),
            ],
        )
}

