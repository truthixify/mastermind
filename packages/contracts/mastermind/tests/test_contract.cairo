    let (mastermind, _) = deploy_contract();
    let mut spy = spy_events();
    let solution_hash = 0x265c6daf938dfc5d71bac7f6512e6fae06a430847ea1175df63b85a5332be1e1;
    let creator_name = 'lukuluku';

